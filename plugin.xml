<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="keyReader" name="Key Reader" schema="schema/keyReader.exsd"/>
   <extension
         point="org.nightlabs.jfire.base.j2ee.remoteResourceFilter">
      <exclusion pattern="org\/nightlabs\/keyreader\/.*"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.nightlabs.keyreader.preference.KeyReaderPreferencePage"
            id="org.nightlabs.keyreader.preference.KeyReaderPreferencePage"
            name="KeyReader Preferences"/>
   </extension>

</plugin>
